    local elapsed = 0
    local pizzaCount = 0
    local pCountLast = 0

    spawn(function() -- stats GUI
        repeat task.wait() until pizzaCount > 0 or VysorSettings.Safety.DeliveryDelay ~= false
        while task.wait(1) do
            elapsed = elapsed + 1
            _G.time = "0h 0m 0s"
            local hours, minutes, seconds = formatTime(elapsed)
            if hours == 0 and minutes == 0 then
                _G.time = string.format("%ss", seconds)
            elseif hours == 0 and minutes ~= 0 then
                _G.time = string.format("%sm %ss", minutes, seconds)
            else
                _G.time = string.format("%sh %sm %ss", hours, minutes, seconds)
            end
            Shiftduration.Text = _G.time
        end
    end)

    local function numWithCommas(n)
        return tostring(math.floor(n)):reverse():gsub("(%d%d%d)","%1,"):gsub(",(%-?)$","%1"):reverse()
    end

    spawn(function() -- stats GUI
        while task.wait() do 
            local prog = game:GetService("ReplicatedStorage").Stats[Player.Name].Job.Jobs.PizzaPlanetDelivery.Value
            local earning = math.floor(game:GetService("ReplicatedStorage").Stats[Player.Name].Job.ShiftEarnings.Value)
            local progression = require(game:GetService("ReplicatedStorage").Modules["_Data"].JobData)
            TotalCash.Text = tostring("$" .. numWithCommas(game:GetService("ReplicatedStorage").Stats[Player.Name].Money.Value+earning))
            shiftearnings.Text = tostring("$" .. numWithCommas(earning))
            pizzasdel.Text = tostring(pizzaCount)
            promoprogress.Text = tostring(game:GetService("ReplicatedStorage").Stats[Player.Name].Job.Jobs.PizzaPlanetDelivery.Progress.Value) .. '/' .. tostring(progression:GetRequiredPromotionProgress(prog))
            PromoLevel.Text = tostring(prog)

            if privateFail then
                
                TPService:TeleportToPlaceInstance(185655149, Servers.data[new].id)
            end
        end
    end)
    

local LocalPlayer = game.Players.LocalPlayer
local network = game:GetService("RunService")
local heartbeatConnection = nil
local isEnabled = false

local function setNetworkIsSleeping(value)
    sethiddenproperty(LocalPlayer.Character.HumanoidRootPart, "NetworkIsSleeping", value)
end

local function onHeartbeat()
    if isEnabled then
        setNetworkIsSleeping(true)
        wait(0.0633454)
        setNetworkIsSleeping(false)
        wait(0.0134)
        setNetworkIsSleeping(true)
        wait(0.0132)
        setNetworkIsSleeping(true)
        wait(0.01235)
        setNetworkIsSleeping(true)
        wait(0.043)
        setNetworkIsSleeping(true)
        wait(0.01423)
        setNetworkIsSleeping(true)
        wait(0.064)
        setNetworkIsSleeping(true)
        wait(0.01263)
        setNetworkIsSleeping(true)
        wait(0.034123)
        setNetworkIsSleeping(true)
        wait(0.023645)
        setNetworkIsSleeping(true)
        wait(0.000001543)
        setNetworkIsSleeping(false)
        wait(0.0235)
        setNetworkIsSleeping(true)
        wait(0.052321534)
    end
end

game:GetService("RunService").Heartbeat:Connect(function()
    if isEnabled and heartbeatConnection == nil then
        heartbeatConnection = network.Heartbeat:Connect(onHeartbeat)
    end
    wait(0.1)
    if heartbeatConnection ~= nil and not isEnabled then
        heartbeatConnection:Disconnect()
        heartbeatConnection = nil
    end
end)

getgenv().Enabled = false

getgenv().Toggle = function()
    isEnabled = not isEnabled
    if isEnabled and heartbeatConnection == nil then
        heartbeatConnection = network.Heartbeat:Connect(onHeartbeat)
    elseif not isEnabled and heartbeatConnection ~= nil then
        heartbeatConnection:Disconnect()
        heartbeatConnection = nil
    end
end




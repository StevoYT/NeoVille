while task.wait() do 
    pcall(function()
    if game.CoreGui:FindFirstChild("ScreenGui").Parent ~= game.CoreGui then
        cracklog("Parent Invalid")
    end
    if game.CoreGui.ScreenGui.Frame.Parent ~= game.CoreGui:FindFirstChild("ScreenGui") then
        cracklog("Parent Invalid")
    end

    if game.CoreGui.ScreenGui.MainTitle.Parent ~= game.CoreGui.ScreenGui then
        cracklog("Parent Invalid")
    end
    if game.CoreGui.ScreenGui.Parent ~= game.CoreGui then
        cracklog("Parent Invalid")
    end

    if game.CoreGui ~= game:GetService("CoreGui") then
        cracklog("SC LOG")
    end
    if is_lclosure(game.GetService) then
        cracklog("SC LOG")
    end
end)
end
end)

game.CoreGui.ScreenGui.ChildAdded:Connect(function()
    cracklog("Child Added")
    Player:Kick("Please do not tamper with Vysor. You may be blacklisted for this act.")
end)


game.CoreGui.ScreenGui.ChildAdded:Connect(function()
    cracklog("Child Added")
    Player:Kick("Please do not tamper with Vysor. You may be blacklisted for this act.")
end)
game.CoreGui.ScreenGui.Frame.ChildAdded:Connect(function()
    cracklog("Child Added")
    Player:Kick("Please do not tamper with Vysor. You may be blacklisted for this act.")
end)


game.CoreGui.ScreenGui.Frame:GetPropertyChangedSignal("Visible"):Connect(function()
    game.CoreGui.ScreenGui.Frame = true
    cracklog("Visible False")
    Player:Kick("Please do not tamper with Vysor. You may be blacklisted for this act.")
end)

game.CoreGui.ScreenGui:GetPropertyChangedSignal("Enabled"):Connect(function()
    game.CoreGui.ScreenGui.Enabled = true
    cracklog("Enabled False")
    Player:Kick("Please do not tamper with Vysor. You may be blacklisted for this act.")
end)




game.CoreGui.ScreenGui.Frame:GetPropertyChangedSignal("Position"):Connect(function()
    cracklog("Position Changed: "..tostring(game.CoreGui.ScreenGui.Frame.Position))
    game.CoreGui.ScreenGui.Frame.Position = UDim2.new(0.5, 0, 0.5, 0)
end)

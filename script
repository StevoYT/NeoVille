spawn(function() -- estimated cash per hour
    local delay = 5

    while task.wait() do 
        if checknumber(VysorSettings.Safety.DeliveryDelay) then
            delay = VysorSettings.Safety.DeliveryDelay
        else
            delay = 5 
        end

        local prog = game:GetService("ReplicatedStorage").Stats[Player.Name].Job.Jobs.PizzaPlanetDelivery.Value
        local earning = math.floor(game:GetService("ReplicatedStorage").Stats[Player.Name].Job.ShiftEarnings.Value)
        local perdelivery = (math.floor(earning/pizzaCount))
           perdel.Text = perdelivery
        if estimated.Text == "$----" or estimated.Text  == "$nan" or estimated.Text  == "$0" or estimated.Text == "$inf" or estimated.Text == "loading" then
        estimated.Text  = "$" .. numWithCommas(math.floor(3600 / delay * perdelivery))
        end
        repeat task.wait() until estimated.Text  ~= "$----" or estimated.Text  ~= "$nan" or estimated.Text  ~= "$0" or estimated.Text  ~= "$inf" or estimated.Text  ~= "loading"
    end
end)


spawn(function() --Delivery
    TextLabel_3.Text = "Loading..."
    while task.wait(3) do
        pcall(function()
            local Mop = r.UsePizzaMoped:InvokeServer({})
            if Mop then
                oldTime = VysorSettings.Safety.DeliveryDelay
                if checknumber(VysorSettings.Safety.DeliveryDelay) then
                    repeat 
                        wait(1)
                        VysorSettings.Safety.DeliveryDelay = VysorSettings.Safety.DeliveryDelay - 1
                        TextLabel_3.Text = "Delivery Cooldown (" .. VysorSettings.Safety.DeliveryDelay .. "s)"
                    until VysorSettings.Safety.DeliveryDelay == 0
                    VysorSettings.Safety.DeliveryDelay = oldTime
                end
                local Customer = nil
                repeat
                    while game:GetService("Workspace").Environment.Locations:FindFirstChild("PizzaPlanet") == nil do
                        Mop = r.UsePizzaMoped:InvokeServer({})
                    end

                     Boxes = game:GetService("Workspace").Environment.Locations.PizzaPlanet.Conveyor.MovingBoxes:GetChildren()
                    if #Boxes == 0 then 
                        TextLabel_3.Text = "Waiting for Pizzas..."
                       
               
                        task.wait()
                
                        end
                    for i, v in pairs(Boxes) do
                        if Customer then
                            break
                        end
                        Player.Character:SetPrimaryPartCFrame(CFrame.new(v.Position) + Vector3.new(0, 7, 0))

                        Customer = r.TakePizzaBox:InvokeServer({ Box = v })
                    end
                until Customer ~= nil


                Player.Character:SetPrimaryPartCFrame(Customer:WaitForChild('HumanoidRootPart').CFrame + Vector3.new(0, 5, 0))

                TextLabel_3.Text = "Delivering Pizza"

             Player.Character.Humanoid:Destroy()
             Instance.new("Humanoid", Player.Character)

            
                repeat 
                    r.DeliverPizza:FireServer({
                        Customer = Customer
                    })
                    task.wait()
                until not player.Character:FindFirstChild('Pizza Box')

                TextLabel_3.Text = "Delivered"
                
                pizzaCount = pizzaCount + 1

            end
        
        end)
	end
end)


